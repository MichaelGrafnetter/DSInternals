<?xml version="1.0" encoding="utf-8" ?>
<asn:Choice
  xmlns:asn="http://schemas.dot.net/asnxml/201808/"
  name="OriginatorIdentifierOrKey"
  namespace="DSInternals.Common.Cryptography.Asn1.Pkcs7">

  <!--
    https://tools.ietf.org/html/rfc5652#section-6.2.2

    OriginatorIdentifierOrKey ::= CHOICE {
        issuerAndSerialNumber IssuerAndSerialNumber,
        subjectKeyIdentifier[0] SubjectKeyIdentifier,
        originatorKey[1] OriginatorPublicKey
    }

    DEFINITIONS IMPLICIT TAGS, so [0] is [0] IMPLICIT, and [1] is [1] IMPLICIT
  -->
  <asn:AsnType name="IssuerAndSerialNumber" typeName="DSInternals.Common.Cryptography.Asn1.Pkcs7.IssuerAndSerialNumber" />
  <asn:OctetString name="SubjectKeyIdentifier" implicitTag="0" />
  <asn:AsnType name="OriginatorKey" typeName="DSInternals.Common.Cryptography.Asn1.Pkcs7.OriginatorPublicKey" implicitTag="1" />
</asn:Choice>
