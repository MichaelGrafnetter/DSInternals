    <View>
      <Name>LapsPasswordInformation</Name>
      <ViewSelectedBy>
        <TypeName>DSInternals.Common.Data.LapsPasswordInformation</TypeName>
      </ViewSelectedBy>
      <TableControl>
        <TableHeaders>
          <TableColumnHeader>
            <Label>ComputerName</Label>
          </TableColumnHeader>
          <TableColumnHeader>
            <Label>Account</Label>
          </TableColumnHeader>
          <TableColumnHeader>
            <Label>Password</Label>
