{
  "profiles": {
    "Debug in PowerShell Desktop 64-bit": {
      "commandName": "Executable",
      "executablePath": "$(WINDIR)\\System32\\WindowsPowerShell\\v1.0\\powershell.exe",
      "commandLineArgs": "-NoExit -NoLogo -NoProfile -ExecutionPolicy Bypass -File Debug.ps1 -ModulePath \"$(TargetDir)..\\DSInternals.psd1\"",
      "workingDirectory": "$(MSBuildProjectDirectory)",
      "nativeDebugging": true
    },
    "Debug in PowerShell Desktop 32-bit": {
      "commandName": "Executable",
      "executablePath": "$(WINDIR)\\SysWOW64\\WindowsPowerShell\\v1.0\\powershell.exe",
      "commandLineArgs": "-NoExit -NoLogo -NoProfile -ExecutionPolicy Bypass -File Debug.ps1 -ModulePath \"$(TargetDir)..\\DSInternals.psd1\"",
      "workingDirectory": "$(MSBuildProjectDirectory)",
      "nativeDebugging": true
    },
    "Debug in PowerShell Core 7": {
      "commandName": "Executable",
      "executablePath": "$(PROGRAMFILES)\\PowerShell\\7\\pwsh.exe",
      "commandLineArgs": "-NoExit -NoLogo -NoProfile -ExecutionPolicy Bypass -File Debug.ps1 -ModulePath \"$(TargetDir)..\\DSInternals.psd1\"",
      "workingDirectory": "$(MSBuildProjectDirectory)",
      "nativeDebugging": true
    }
  }
}
